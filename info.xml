<?xml version="1.0"?>
<extension key="com.joineryhq.jsumfields" type="module">
  <file>jsumfields</file>
  <name>Joinery's More Summary Fields</name>
  <description>Provides additional fields under the Summary Fields framework</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.jsumfields</url>
    <url desc="Documentation">https://github.com/twomice/com.joineryhq.jsumfields</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.jsumfields/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-20</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <requires>
    <ext>net.ourpowerbase.sumfields</ext>
  </requires>
  <comments>Addes a large number of available Summary Fields, configurable in the Summary Fields Settings form.</comments>
  <civix>
    <namespace>CRM/Jsumfields</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Jsumfields_Upgrader</upgrader>
</extension>
